var sounds = {
   49 : 'privacidad-integral-clientes-y-contacto', // key '1'
   50 : 'privacidad-integral-empleados', // key 2
   51 : 'privacidad-integral-candidatos', //key 3
   52 : 'contrato', //key 4
   53 : 'codigo-practicas', //key 5
   32 : 'inicio'
};
document.onkeydown = function(e) {
	document.getElementById('inicio').load();
	document.getElementById('privacidad-integral-clientes-y-contacto').load();
	document.getElementById('privacidad-integral-empleados').load();
	document.getElementById('privacidad-integral-candidatos').load();
	document.getElementById('contrato').load();
	document.getElementById('codigo-practicas').load();
    var soundId = sounds[e.keyCode];
    if (soundId) document.getElementById(soundId).play();
    else console.log("key not mapped : code is", e.keyCode);
}
    
function playAudio() { 
    inicio.play(); 
    document.getElementById('privacidad-integral-clientes-y-contacto').load();
	document.getElementById('privacidad-integral-empleados').load();
	document.getElementById('privacidad-integral-candidatos').load();
	document.getElementById('contrato').load();
	document.getElementById('codigo-practicas').load();
    play.style.display = "none";
    pause.style.display = "block";
} 

function pauseAudio() { 
    inicio.pause(); 
    play.style.display = "block";
    pause.style.display = "none";
} 

</script>
